<script type="text/javascript">
    $(document).ready(function() {
        $(".btn").removeClass("Next");
        $(".btn").removeClass("Previous");
        $($(".btn").get($(".btn").index($(".active")) + 1)).addClass("Next");
        if ($(".btn").index($(".active")) - 1 >= 0) {
            $($(".btn").get($(".btn").index($(".active")) - 1)).addClass("Previous");
        }
        $('.optionSelector a').click(function(e) {
            e.preventDefault();
            e.stopPropagation();
            $(window).trigger('loadmeeeee');
            if ((typeof wistiaEmbeds !== 'undefined')) {
                for (var i = 0; i < wistiaEmbeds.length; i++) {
                    wistiaEmbeds[i].pause();
                }
            }
            var parallax = $(this).closest('.parallax');
            var option = parallax.data('option') || 'create';
            var newOption = $(this).data('option');
            parallax.removeClass(option).addClass(newOption).data('option', newOption);
            $(this).siblings().removeClass('active');
            $(this).addClass('active');
            $(".btn").removeClass("Next");
            $(".btn").removeClass("Previous");
            $($(".btn").get($(".btn").index($(".active")) + 1)).addClass("Next");
            if ($(".btn").index($(".active")) - 1 >= 0) {
                $($(".btn").get($(".btn").index($(".active")) - 1)).addClass("Previous");
            }
        });
    });
  </script>
